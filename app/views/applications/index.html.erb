
<div class="max-w-7xl mx-auto mt-12">
  <%= render "app_custom_view", apps: @apps, org_id: @org_id %>
</div>



<p style="color: green"><%= notice %></p>

<h1>Applications</h1>

<div id="applications">
  <% @applications.each do |application| %>
    <%= render application %>
    <p>
      <%= link_to "Show this application", application %>
    </p>
  <% end %>
</div>

<%= link_to "New application", new_application_path %>
